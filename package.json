    "verify-readme-up-to-date": "node scripts/verify-readme-up-to-date.js"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "circular-buffer": "^1.0.2",
    "execa": "^4.0.0",
    "fs-extra": "^9.0.0",
    "got": "^10.7.0",
    "simple-node-logger": "^18.12.24"
  },
  "devDependencies": {
    "lodash.isequal": "^4.5.0",
    "markdown-table": "^2.0.0",
    "mastarm": "^5.3.1",
    "nock": "^12.0.3"
  }
